<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SeeDeliveryManFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:background="@drawable/newbck"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.SeeDeliveryManActivity">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:text="Delivery Man Data"
            android:textColor="@color/purple_700"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.139"
            tools:layout_editor_absoluteX="-16dp" />

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:background="@drawable/newet"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="50dp"
            android:layout_marginTop="36dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
            tools:layout_editor_absoluteX="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:layout_gravity="left"
                android:paddingLeft="5dp"
                android:text="Location: "
                android:textColor="@color/purple_700"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/see_loc"
                android:layout_width="250dp"
                android:textSize="16sp"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:paddingLeft="5dp"

                android:textColor="@color/black"
                tools:layout_editor_absoluteX="104dp"
                tools:layout_editor_absoluteY="144dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_marginTop="28dp"
            android:background="@drawable/newet"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_height="50dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            tools:layout_editor_absoluteX="26dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:paddingLeft="5dp"
                android:text="Name: "
                android:textColor="@color/purple_700"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/see_name"
                android:layout_width="250dp"
                android:textSize="16sp"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:paddingLeft="5dp"
                android:textColor="@color/black"
                tools:layout_editor_absoluteX="104dp"
                tools:layout_editor_absoluteY="144dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="28sp"
            android:background="@drawable/newet"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center"

            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
            tools:layout_editor_absoluteX="-65dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:paddingLeft="5dp"
                android:text="Distance: "
                android:textColor="@color/purple_700"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/see_distance"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:gravity="center|left"
                android:paddingLeft="5dp"

                android:textColor="@color/black"
                android:textSize="16sp"
                tools:layout_editor_absoluteX="104dp"
                tools:layout_editor_absoluteY="144dp" />
        </LinearLayout>

        <Button
            android:id="@+id/showbtn"
            android:layout_width="139dp"
            android:layout_height="57dp"
            android:text="SHOW!"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>